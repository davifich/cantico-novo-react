{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    },
    "module":"esnext",
    "moduleResolution": "bundler",
    "jsx": "react-native"
  },
  "include": [
    "app/**/*.ts",
    "app/**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "expo-file-system"
  ],
  "exclude": [
    "backend",
    "node_modules"
  ]
  
}
